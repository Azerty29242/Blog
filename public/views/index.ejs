<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@materializecss/materialize@1.2.1/dist/css/materialize.min.css">
    <title>Blog</title>
</head>
<body>
  <div class="container" id="main">
    <h1>Blog</h1>
    <ul class="collection" id="articles">
      <% items.forEach(item => { %>
        <li class="collection-item">
          <h4><a style="all: unset" href="<%= item.link %>"><%= item.title %></a></h4>
          <p><%= item.content %></p>
          <i><%= new Date(item.pubDate).toLocaleDateString() %></i>
        </li>
      <% }) %>
    </ul>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/@materializecss/materialize@1.2.1/dist/js/materialize.min.js"></script>
  <script src="js/script.js"></script>
  <script>
    console.log("<%= items.map(item => item.title) %>")
  </script>
</body>
</html>